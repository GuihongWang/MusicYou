<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="108dp"
            android:height="108dp"
            android:viewportWidth="108"
            android:viewportHeight="108">
            <group
                android:name="back"
                android:pivotX="54"
                android:pivotY="54"
                android:scaleX="0"
                android:scaleY="0">
                <path
                    android:pathData="M52 36, L74 36"
                    android:strokeWidth="8"
                    android:strokeColor="@android:color/system_neutral1_100" />
                <path
                    android:pathData="M56 32, L56 64"
                    android:strokeWidth="8"
                    android:strokeColor="@android:color/system_neutral1_100" />
                <path
                    android:fillColor="@android:color/system_neutral1_100"
                    android:pathData="M 36, 64 a 12,12 0 1,1 24,0 a 12,12 0 1,1 -24,0" />
            </group>
            <group
                android:name="front"
                android:pivotX="54"
                android:pivotY="54"
                android:scaleX="0"
                android:scaleY="0">
                <path
                    android:pathData="M52 36, L74 36"
                    android:strokeWidth="8"
                    android:strokeColor="@android:color/system_accent1_200" />
                <path
                    android:pathData="M56 32, L56 64"
                    android:strokeWidth="8"
                    android:strokeColor="@android:color/system_accent1_300" />
                <path
                    android:fillColor="@android:color/system_accent1_500"
                    android:pathData="M 36, 64 a 12,12 0 1,1 24,0 a 12,12 0 1,1 -24,0" />
            </group>
        </vector>
    </aapt:attr>
    <target android:name="back">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="scaleX"
                    android:valueFrom="0"
                    android:valueTo="1.2"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="scaleY"
                    android:valueFrom="0"
                    android:valueTo="1.2"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="front">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="300"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="scaleX"
                    android:startOffset="200"
                    android:valueFrom="0"
                    android:valueTo="1.2"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="300"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="scaleY"
                    android:startOffset="200"
                    android:valueFrom="0"
                    android:valueTo="1.2"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
</animated-vector>
